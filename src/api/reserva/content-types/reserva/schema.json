      "required": true
    },
    "pedidos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pedido.pedido",
      "mappedBy": "reserva"
    },
    "mesa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mesa.mesa",
      "mappedBy": "reserva"
    }
